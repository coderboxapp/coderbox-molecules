{
  "presets": [
    "react",
    "flow",
    [
      "env",
      {
        "targets": {
          "browsers": [ "last 2 versions" ]
        }
      }
    ]
  ],
  "plugins": [
    [ "transform-object-rest-spread" ],
    [ "babel-plugin-transform-class-properties" ],
    [ "styled-components" ],
    [
      "module-resolver",
      {
        "root": [ "./src" ]
      }
    ]
  ]
}
